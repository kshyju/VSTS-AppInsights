{
  "manifestVersion": 1,
  "id": "AppInsightsExt",
   "baseUri": "https://localhost/AppInsights",
  "version": "1.0.7",
  "name": "Insights",
  "description": "See insights",
  "icons": {
    "default": "img/logo.png"
  },
  "categories": [
    "Plan and track"
  ],
  "content": {
        "details": {
            "path": "overview.md"   
        }
    },
    "links": {
        "support": {
            "uri": "https://www.twitter.com/kshyju"
        }
    },
  "tags": [
    "code",
    "change sets",
    "App Insights",
    "Insights"
  ],
  "branding": {
    "color": "#E6EAED"
  },
  "screenshots": [
    {
      "path": "img/RecentCodeCommitsScreenShot.png"
    }
  ],
  "public": true,
  "publisher": "kshyju",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "AppInsightsWidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        "kshyju.AppInsightsExt.AppInsightsWidgetConfiguration"
      ],
      "properties": {
        "name": "App Insights",
        "description": "Simple widget to show the most recent code commits",
        "catalogIconUrl:": "img/logo.png",
        "previewImageUrl": "img/logo.png",
        "uri": "index.html",
        "supportedSizes": [
          {
            "rowSpan": 2,
            "columnSpan": 2
          },
          {
            "rowSpan": 3,
            "columnSpan": 2
          },
          {
            "rowSpan": 3,
            "columnSpan":3
          }
        ],
        "supportedScopes": [ "project_team" ]
      }
    }
    , {
      "id": "AppInsightsWidgetConfiguration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [ "ms.vss-dashboards-web.widget-configuration" ],
      "properties": {
        "name": "AppInsights Widget Configuration",
        "description": "Configures HelloWorldWidget",
        "uri": "configuration.html"
      }
}
  ],
  "files": [
    {
      "path": "index.html",
      "addressable": true
    },
    {
      "path": "configuration.html",
      "addressable": true
    },
    {
      "path": "sdk/scripts",
      "addressable": true
    },    
    {
      "path": "src/main.js",
      "addressable": true
    }
    ,    
    {
      "path": "src/AppInsights.js",
      "addressable": true
    }
    ,
    {
      "path": "src/AppInsightsWidgetConfiguration.js",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    }
  ],
  "scopes": [
    "vso.work",
    "vso.code"
  ]
}